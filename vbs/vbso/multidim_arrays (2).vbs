'================================================================================
' rectangular array
'================================================================================
dim r( 1, 1 )
    r( 0, 0 ) = 11
    r( 0, 1 ) = 12
    r( 1, 0 ) = 21
    r( 1, 1 ) = 22

for i = lbound( r ) to ubound( r ) ' or ( r, 1 )
    for j = lbound( r, 2 ) to ubound( r, 2 )
        wscript.stdout.write r( i, j ) & " "
    next
    wscript.stdout.writeline
next

wscript.stdout.writeline

'================================================================================
' jagged array
'================================================================================
dim j
j = array                        _
    (                            _
        array( 11             ), _
        array( 21, 22         ), _
        array( 31, 32, 33     ), _
        array( 41, 42, 43, 44 )  _
    )

for n = lbound( j ) to ubound( j )
    for m = lbound( j( 0 ) ) to ubound( j( n ) )
        wscript.stdout.write j( n )( m ) & " "
    next
    wscript.echo
next

'================================================================================
' adjacency matrix
'================================================================================
dim ad( 4, 4 )
    ad( 0, 0 ) = 0: ad( 0, 1 ) = 1: ad( 0, 2 ) = 0: ad( 0, 3 ) = 0: ad( 0, 4 ) = 1
    ad( 1, 0 ) = 1: ad( 1, 1 ) = 0: ad( 1, 2 ) = 0: ad( 1, 3 ) = 1: ad( 1, 4 ) = 0
    ad( 2, 0 ) = 1: ad( 2, 1 ) = 0: ad( 2, 2 ) = 1: ad( 2, 3 ) = 0: ad( 2, 4 ) = 1
    ad( 3, 0 ) = 0: ad( 3, 1 ) = 1: ad( 3, 2 ) = 0: ad( 3, 3 ) = 0: ad( 3, 4 ) = 0
    ad( 4, 0 ) = 0: ad( 4, 1 ) = 0: ad( 4, 2 ) = 1: ad( 4, 3 ) = 0: ad( 4, 4 ) = 1
